{
  "extends": ["react-native", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "printWidth": 100,
        "parser": "babylon",
        "bracketSpacing": false,
        "tabWidth": 2,
        "useTabs": false,
        "jsxBracketSameLine": false
      }
    ],
    "react/prop-types": 0
  }
}